  `etat` varchar(10) NOT NULL,
  `etatprix` varchar(40) NOT NULL default 'no',
  `prixktcentrex` varchar(30) NOT NULL,
  `etatprix2` varchar(15) NOT NULL default 'off',
  `prixrevente` varchar(30) NOT NULL,
  `sauv` int(5) NOT NULL default '0',
  `dst1` varchar(80) NOT NULL,
  KEY `calldate` (`calldate`),
  KEY `dst` (`dst`),
  KEY `accountcode` (`accountcode`),
  KEY `numero` (`numero`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=2 ;

