-- Structure de la table `facture`
--

CREATE TABLE IF NOT EXISTS `facture` (
  `id` int(11) NOT NULL auto_increment,
  `idclient` int(11) NOT NULL,
  `idclientSansCpt` int(5) NOT NULL,
  `date` datetime NOT NULL,
  `paiement` tinyint(1) NOT NULL default '0',
  `methode` tinyint(1) NOT NULL default '3',
  `tva` decimal(5,5) NOT NULL default '0.19600',
  `liv_adresse` varchar(255) NOT NULL,
  `liv_codepostal` varchar(30) NOT NULL,
  `liv_ville` varchar(100) NOT NULL,
  `liv_pays` varchar(100) NOT NULL,
  `date_paiement` datetime default NULL,
  `date_livraison` datetime default NULL,
  `frais` decimal(7,2) NOT NULL default '0.00',
  `idabonnement` int(11) default NULL,
  `prorata` decimal(6,5) NOT NULL default '1.00000',
  `prorata_debut` date default NULL,
  `prorata_fin` date default NULL,
  PRIMARY KEY  (`id`),
  KEY `idclient` (`idclient`),
  KEY `paiement` (`paiement`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=2513 ;

