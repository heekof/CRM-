  `format` varchar(50) NOT NULL,
  `semaine` int(2) NOT NULL,
  PRIMARY KEY  (`numpub`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=13 ;

