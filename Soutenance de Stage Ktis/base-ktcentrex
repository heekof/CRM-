-- Structure de la table `compte`
--

CREATE TABLE IF NOT EXISTS `compte` (
  `id` bigint(20) NOT NULL auto_increment,
  `numero` int(20) NOT NULL,
  `password` varchar(16) NOT NULL,
  `idfacture` int(11) NOT NULL,
  `server` varchar(128) NOT NULL,
  `codec` varchar(64) NOT NULL,
  `destination` varchar(255) NOT NULL,
  `ajout` varchar(50) NOT NULL,
  `idclient` int(20) NOT NULL,
  `etatcredit` varchar(20) NOT NULL,
  `offre` varchar(30) NOT NULL,
  `etat` varchar(50) NOT NULL,
  `clietranger` int(23) NOT NULL,
  `etat2` varchar(30) NOT NULL,
  `trunk` int(11) NOT NULL default '1',
  `etat3` int(20) NOT NULL default '0',
  `did` int(15) NOT NULL,
  `forcrm` enum('no','yes') NOT NULL default 'no',
  `letype` varchar(15) NOT NULL default 'sip',
  `choixserveur` varchar(15) NOT NULL,
  `terminaison` int(5) NOT NULL default '1',
  `etat4` varchar(5) NOT NULL default '0',
  `firewall` varchar(30) NOT NULL,
  `ip` varchar(80) NOT NULL,
  PRIMARY KEY  (`id`),
  KEY `numero` (`numero`),
  KEY `idfacture` (`idfacture`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=5521 ;

